# EditorConfig is awesome: https://EditorConfig.org

# top-most EditorConfig file
root = true

[*]
indent_style = space
indent_size = 2
end_of_line = lf
charset = utf-8
# Tirar espa√ßo branco ao fim da linha
trim_trailing_whitespace = true
# Adicionar uma linha no final de cada arquivo
insert_final_newline = true
